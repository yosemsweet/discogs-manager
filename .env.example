# Discogs API Configuration
DISCOGS_API_TOKEN=your_discogs_personal_access_token_here
DISCOGS_USERNAME=your_discogs_username

# SoundCloud OAuth 2.1 Configuration
# Get your credentials from: https://soundcloud.com/you/apps
SOUNDCLOUD_CLIENT_ID=your_soundcloud_client_id
SOUNDCLOUD_CLIENT_SECRET=your_soundcloud_client_secret

# These are automatically populated after running: npm run dev -- auth
SOUNDCLOUD_ACCESS_TOKEN=automatically_set_by_oauth_flow
SOUNDCLOUD_REFRESH_TOKEN=automatically_set_by_oauth_flow
# Logging
LOG_LEVEL=info
